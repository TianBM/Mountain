{{!< default}}
{{#post}}
<!-- START: HOME -->
<section class="section-home " id="home">
	<div class="glass">
		<h3>{{@site.title}}</h3>
		<p class="post-header" id="index_djt"></p>
		</div>
	<img src="{{#if feature_image}}{{img_url feature_image}}{{else}}{{asset "/images/banner.jpg"}}{{/if}}" id="full_img" />
</section>
<!-- END: HOME -->
<!-- START: CONTACT -->
<section class="section section-light section-contact" id="contact">
	<div class="container">
		<div class="section-head">
			<h2>{{title}}</h2>
		</div>
	</div>
	{{> post-info}}
	<div class="container">
		<div class="post-content {{post_class}}">
			<!-- content -->
			{{content}}
		</div>
	</div>
	{{> comment}}
</section>
{{/post}}
<!-- END: CONTACT -->